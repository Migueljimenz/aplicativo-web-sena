<section class="InicioSesion">
  <article class="articulo1">
    <h2>Iniciar Sesion</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="inicio">
    <%= f.email_field :email, autofocus: true, autocomplete: "email",placeholder:"Ingrese su email" ,id:"inputEmail"%>
  </div>

  <div class="inicio">
    <%= f.password_field :password, autocomplete: "current-password",placeholder:"ingrese su contraseÃ±a", id:"inputPassword"%>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="inicio">
      <%= f.check_box :remember_me %>
      <%= f.label :recuerdame, id:"inicio"%>
    </div>
  <% end %>

  <div class="botonInicio">
    <%= f.submit "Iniciar Sesion", id:"botonInicio"%>
  </div>
<% end %>

<%= render "devise/shared/links" %>

  </article>
  <article  class="articulo2">
    <%= image_tag "logo-sena.png", id:"logo"%>
  </article>
</section>